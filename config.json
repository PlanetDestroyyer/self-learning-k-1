{
  "model": {
    "vocab_size": 256,
    "embed_dim": 128,
    "hidden_dim": 256,
    "num_heads": 4,
    "ff_dim": 512,
    "max_seq_len": 64
  },
  "k1_system": {
    "hierarchy": {
      "depth": 3,
      "branching_factor": 4,
      "agents_per_level": [1, 4, 16]
    },
    "routing": {
      "top_k": 4,
      "exploration_rate": 0.1,
      "exploration_decay": 0.995
    },
    "trust": {
      "initial_trust": 0.5,
      "decay_rate": 0.01,
      "min_trust": 0.1,
      "max_trust": 1.0
    },
    "credit_assignment": {
      "top_k_agents": 5,
      "trust_weight": 0.5
    }
  },
  "training": {
    "learning_rate": 0.0001,
    "batch_size": 1,
    "num_epochs": 3,
    "max_steps": 10000,
    "eval_every": 500,
    "log_every": 100,
    "gradient_clip": 1.0
  },
  "phase1": {
    "steps": 5000,
    "fixed_parameters": true,
    "structural_changes": false,
    "description": "Learn basic patterns with fixed architecture"
  },
  "phase2": {
    "steps": 5000,
    "autonomous_parameters": true,
    "structural_changes": true,
    "prune_threshold": 0.2,
    "merge_threshold": 0.85,
    "grow_threshold": 0.9,
    "description": "Enable autonomous learning and structural plasticity"
  },
  "structural": {
    "pruning": {
      "enabled": true,
      "trust_threshold": 0.2,
      "min_agents": 5,
      "check_interval": 500
    },
    "merging": {
      "enabled": true,
      "similarity_threshold": 0.85,
      "check_interval": 500
    },
    "growing": {
      "enabled": true,
      "performance_threshold": 0.9,
      "max_new_agents": 3,
      "check_interval": 1000
    },
    "reorganization": {
      "enabled": true,
      "check_interval": 2000
    }
  },
  "autonomy": {
    "parameter_control": {
      "enabled": true,
      "lr_adjustment_rate": 0.1,
      "min_lr": 1e-6,
      "max_lr": 0.01
    },
    "early_stopping": {
      "enabled": true,
      "patience": 1000,
      "min_improvement": 0.001
    }
  },
  "safety": {
    "snapshot_interval": 1000,
    "max_snapshots": 5,
    "rollback_threshold": 0.5
  },
  "data": {
    "dataset": "tiny_shakespeare",
    "train_split": 0.9,
    "val_split": 0.05,
    "test_split": 0.05
  },
  "comparison": {
    "metrics": ["loss", "perplexity", "accuracy", "generation_quality"],
    "num_generation_samples": 5,
    "generation_length": 100
  }
}
